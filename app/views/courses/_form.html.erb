<div class='container' style="width: 50%">
<%= simple_form_for @course,html: { class: 'form-horizontal' } do |f| %>
  <%= f.input :title, label: 'Course Title' %>
  <%= f.input :description %>
  <%= f.input :start_date %>
  <%= f.input :end_date %>

  <%= f.submit class: 'btn btn-primary' %>
 <% end %>
</div>
