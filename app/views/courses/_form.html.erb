<%= simple_form_for @course,html: { class: 'form-horizontal' } do |f| %>
  <%= f.input :title, label: 'Course Title' %>
  <%= f.input :description %>
  <%= f.input :start_date %>
  <%= f.input :end_date %>
  <%= f.input :instructor_id, collection: @instructor.map{|i|[i.first_name, i.id]} %>
  <%= f.submit class: 'btn btn-primary' %>
 <% end %>